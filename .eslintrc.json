{
    "env": {
        "browser": true,
        "node": true,
        "es2021": true
    },
    "extends": ["airbnb-base", "prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "import/extensions": [
      "error",
      {
          "js": "ignorePackages"
      }
    ],
      "no-unused-vars" : "warn",
      "no-console" :"off",
      "func-names" : "off",
      "object-shorthand" : "off",
      "class-methods-use-this" : "off"
  }
    }
}
